        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="BS_FPOS_AccDayFile_BuildAccDayReportFile_${__threadGroupName}" enabled="true">
          <stringProp name="BeanShellSampler.query">// Define the path to the existing file
String filePath = &quot;../../TestData/FPOS_Templates/Prod/AccDay_Report.txt&quot;;
String STID = vars.get(&quot;StoreID&quot;);

// Define the base path for the output files
String baseOutputPath = &quot;../../TestResults/&quot; + STID + &quot;/&quot;;
File storeIDFolder = new File(baseOutputPath);
storeIDFolder.mkdirs();

String counter = &quot;$(__counter(FALSE,0000))&quot;;
String OPEN = vars.get(&quot;PrevTimeStamp&quot;); 
String CLOS = vars.get(&quot;CurrTimeStamp&quot;); 
String PER = vars.get(&quot;AccDayNum&quot;); 
String STAP = vars.get(&quot;AccNextDayNum&quot;);
String CLSP = vars.get(&quot;AccNextDayNum&quot;);
String STAS1 = vars.get(&quot;T1ShiftID&quot;);
String CLSS1 = vars.get(&quot;T1ShiftID&quot;);
String STAS2 = vars.get(&quot;T2ShiftID&quot;);
String CLSS2 = vars.get(&quot;T2ShiftID&quot;);
String REP_NR = vars.get(&quot;AccDayNum&quot;);

// Read the file content
String fileContent = &quot;&quot;;
try {
    BufferedReader reader = new BufferedReader(new FileReader(filePath));
    String line;
    while ((line = reader.readLine()) != null) {
        fileContent += line + &quot;\n&quot;;
    }
    reader.close();
} catch (Exception e) {
    log.error(&quot;Error reading file: &quot; + e);
}

// Replace dynamic values
fileContent = fileContent.replace(&quot;###OPEN###&quot;, OPEN);
fileContent = fileContent.replace(&quot;###CLOS###&quot;, CLOS);
fileContent = fileContent.replace(&quot;###PER###&quot;, PER);
fileContent = fileContent.replace(&quot;###STAP###&quot;, STAP);
fileContent = fileContent.replace(&quot;###CLSP###&quot;, CLSP);
fileContent = fileContent.replace(&quot;###STAS1###&quot;, STAS1);
fileContent = fileContent.replace(&quot;###CLSS1###&quot;, CLSS1);
fileContent = fileContent.replace(&quot;###STAS2###&quot;, STAS2);
fileContent = fileContent.replace(&quot;###CLSS2###&quot;, CLSS2);
fileContent = fileContent.replace(&quot;###REP_NR###&quot;, REP_NR);

String BuildAccDayFile = baseOutputPath + STID + (&quot;0&quot; + PER) + &quot;.A&quot;;
String BuildAccDayFilename = STID + (&quot;0&quot; + PER) + &quot;.A&quot;;
vars.put(&quot;AccReportFile&quot;, BuildAccDayFilename);

// Write the modified content to a new file
try {
    FileWriter writer = new FileWriter(BuildAccDayFile);
    writer.write(fileContent);
    writer.close();
    log.info(&quot;File saved successfully: &quot; + BuildAccDayFile);
} catch (Exception e) {
    log.error(&quot;Error writing file: &quot; + e);
}

