            <stringProp name="BeanShellSampler.query">import org.apache.jmeter.services.FileServer;
import java.io.*;
import java.math.BigDecimal;

// Initialize variables for transaction totals
Double transactionTotalT1 = 0.0;
Double transactionTotalT2 = 0.0;

// Get the serial number (sno) and transaction number (trasno) from JMeter variables
String snoString = vars.get(&quot;sno&quot;);
int sno = (snoString != null) ? Integer.parseInt(snoString) : 0;

String trasnoString = vars.get(&quot;trasno&quot;);
int trasno = trasnoString != null ? Integer.parseInt(trasnoString) : 101;

// Get data from dynamic transaction values from Transaction_Parameters pre-processor
String dateTimeTrx = vars.get(&quot;DateTimeTrx&quot;);
String StoreID = &quot;${StoreID}&quot;;
String AccDayNum = vars.get(&quot;AccDayNum&quot;);
String SEQ = vars.get(&quot;AccNextDayNum&quot;);

// Define folder paths for transactions and test results
String folderPath = &quot;../../TestData/FPOS_Templates/Prod/transactions/&quot;;

String counter = &quot;${counter}&quot;;
String counter1 = (&quot;0000&quot; + counter).substring(counter.length());

String storeIDFolderPath = &quot;../../TestResults/&quot; + StoreID + &quot;/&quot;;
File storeIDFolder = new File(storeIDFolderPath);
storeIDFolder.mkdirs();

// Define file paths for transaction build
String BuildFileTrx = storeIDFolderPath + (&quot;0000&quot; + counter).substring(counter.length()) + &quot;0&quot; + &quot;${AccNextDayNum}&quot; + &quot;.TRX&quot;;
String BuildFilename = (&quot;0000&quot; + counter).substring(counter.length()) + &quot;0&quot; + &quot;${AccNextDayNum}&quot; + &quot;.TRX&quot;;
vars.put(&quot;btrxfile&quot;, BuildFilename);

String TransactionCount = vars.get(&quot;ppTrnxNum&quot;);
int count = Integer.parseInt(TransactionCount);

File folder = new File(folderPath);
File[] files = folder.listFiles();

if (files != null &amp;&amp; files.length &gt; 0) {
    
    // Write transactions to a file
    BufferedWriter writer = new BufferedWriter(new FileWriter(BuildFileTrx));

    // Write file header
    writer.write(&quot;[START_FILE]&quot;);
    writer.newLine();
    writer.newLine();
    writer.write(&quot;[TRX]&quot;);
    writer.newLine();

    String shiftNum = &quot;&quot;;
    String operatorId = &quot;&quot;;
    String tillNum = &quot;&quot;;

    // Iterate over transaction files
    for (int i = 0; i &lt; count &amp;&amp; i &lt; files.length; i++)
    {         
        // Determine shift, operator, and till numbers based on iteration
        if ((i % 2 == 0))
        {
            shiftNum = &quot;${T2ShiftID}&quot;;
            operatorId = &quot;${T2OptrID}&quot;;
            tillNum = &quot;${T2TillID}&quot;;
        }
        else
        {
            shiftNum = &quot;${T1ShiftID}&quot;;
            operatorId = &quot;${T1OptrID}&quot;;
            tillNum = &quot;${T1TillID}&quot;;
        }

        File selectedFile = files[i];       
        if (i &gt; 0) sno = Integer.parseInt(vars.get(&quot;sno&quot;));
        
        String s = &quot;&quot;;
        for (int j = 0; j&lt;7- (String.valueOf(i+sno).length()); j++)
            s += &quot;0&quot;;

        int newSeq = sno + i + 1;
        String seqString = String.valueOf(newSeq);
        int length = seqString.length();
        
        if (length &lt; 4) {
            StringBuilder paddedSeq = new StringBuilder();
            for (int k = 0; k &lt; 4 - length; k++) {
                paddedSeq.append(&quot;0&quot;);
            }
            paddedSeq.append(seqString);
            seqString = paddedSeq.toString();
        }

        BufferedReader reader = new BufferedReader(new FileReader(selectedFile));
        String line;
    
        // Process each line of the transaction file
        while ((line = reader.readLine()) != null) 
        { 
            // Replace placeholders with actual values
            line = line.replace(&quot;#SNO#&quot;, String.valueOf(trasno));
            line = line.replace(&quot;#DATI#&quot;, dateTimeTrx);
            line = line.replace(&quot;#STATION_NR#&quot;, StoreID);
            line = line.replace(&quot;#ShiftID#&quot;, shiftNum);
            line = line.replace(&quot;#ACC_REP_NR#&quot;, AccDayNum);
            line = line.replace(&quot;#OPID#&quot;, operatorId);
            line = line.replace(&quot;@TillID@&quot;, tillNum);
            line = line.replace(&quot;#AccDayNum#&quot;, &quot;0&quot; + AccDayNum);
            line = line.replace(&quot;#REFSEQ#&quot;, seqString);
            line = line.replace(&quot;#SEQ#&quot;, &quot;0&quot; + SEQ + tillNum + s + (sno+1-(i/2)));
            
            // Write the modified line to the output file
            writer.write(line);
            writer.newLine();
            
            // Update transaction totals if SLSTOT is encountered
            if (line.startsWith(&quot;SLSTOT&quot;)) {
                String[] parts = line.split(&quot;=&quot;);
                if (parts.length == 2) {
                    try {   
                        if (tillNum.contains(&quot;11&quot;))     
                        {
                            transactionTotalT1  = transactionTotalT1 + Double.parseDouble(parts[1].trim());   
                        }
                        
                        if (tillNum.contains(&quot;12&quot;))                     
                            transactionTotalT2  = transactionTotalT2 + Double.parseDouble(parts[1].trim());
                        
                    } 
                    catch (NumberFormatException e) {
                        log.error(&quot;Error parsing SLSTOT value: &quot; + e.getMessage());
                    }
                }
            }
        }
        
        reader.close();

        // Increment sno and trasno for the next file
        sno++;
        trasno++;
        vars.put(&quot;sno&quot;, String.valueOf(sno));
    }

    // Update transaction totals in JMeter variables
    Double sumT1Dval = Double.parseDouble(vars.get(&quot;sumT1&quot;));
    sumT1Dval = sumT1Dval + transactionTotalT1;
    sumT1Dval = Math.round(sumT1Dval * 100.0) / 100.0;
    vars.put(&quot;sumT1&quot;, Double.toString(sumT1Dval));

    Double sumT2Dval = Double.parseDouble(vars.get(&quot;sumT2&quot;));
    sumT2Dval = sumT2Dval + transactionTotalT2;
    sumT2Dval = Math.round(sumT2Dval * 100.0) / 100.0;
    vars.put(&quot;sumT2&quot;, Double.toString(sumT2Dval));

    vars.put(&quot;trasno&quot;, String.valueOf(trasno));
    vars.put(&quot;sno&quot;, String.valueOf(sno));
    
    // Write file footer and additional information
    writer.write(&quot;[END_TRX]&quot;);
    writer.newLine();
    writer.newLine();
    writer.write(&quot;[GENERIC_FILE_INFO]&quot;);
    writer.newLine();
    writer.write(&quot;CREATED=&quot; + &quot;${__time(yyyyMMddHHmmss)}&quot;);
    writer.newLine();
    writer.newLine();
    writer.write(&quot;[END_GENERIC_FILE_INFO]&quot;);
    writer.newLine();
    writer.newLine();
    writer.write(&quot;[END_FILE]&quot;);
    writer.newLine();

    // Close the file writer
    writer.close();
}</stringProp>
