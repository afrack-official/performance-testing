        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="BS_FPOS_TRX_BuildParamaters_${__threadGroupName}" enabled="true">
          <stringProp name="BeanShellSampler.query">import java.text.SimpleDateFormat;
import java.util.Calendar;
import java.time.LocalDate;
import java.time.temporal.ChronoUnit;
import org.apache.commons.lang3.RandomUtils;
import java.util.Random;

// AccDayNum - Start
String sequenceVar = &quot;AccDayNum&quot;;
String lastDateVar = &quot;lastDate&quot;;
String iterationVar = &quot;iterationCount&quot;;

// Check if the iteration count variable exists, initialize it to 0 if not
int iterationCount = Integer.parseInt(vars.get(iterationVar) != null ? vars.get(iterationVar) : &quot;0&quot;);

// Increment the iteration count
iterationCount++;

// Get the last recorded date
String lastDateStr = props.get(lastDateVar);
LocalDate lastDate = lastDateStr != null ? LocalDate.parse(lastDateStr) : LocalDate.now();

// Get the current date
LocalDate currentDate = LocalDate.now();

// Calculate the number of days between the last recorded date and the current date
long daysDifference = ChronoUnit.DAYS.between(lastDate, currentDate);

// Set the daySequence based on the day difference and iteration count
int initialSequence = 205 + iterationCount - 1; // Increment by 1 for every iteration

int AccDayNum = initialSequence + (int) daysDifference;

// Save the updated sequence number, last recorded date, and iteration count back to JMeter variables and properties
vars.put(sequenceVar, String.valueOf(AccDayNum));
vars.put(iterationVar, String.valueOf(iterationCount));
props.put(lastDateVar, currentDate.toString());
// AccDayNum - End

// TransRefNum - Start
String accDayNumString = vars.get(&quot;AccDayNum&quot;);
if (accDayNumString != null) {
    int accDayNum = Integer.parseInt(accDayNumString);
    int newVariableAccRef = accDayNum + 1;
    vars.put(&quot;AccNextDayNum&quot;, String.valueOf(newVariableAccRef));
} else {
