        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Deleting TRX_H1_H2_A_P" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import com.jcraft.jsch.*

// SFTP server credentials
String host = &quot;prl-d7-sftp.internal.pos.pditechnologies.com&quot;
int port = 22
String username = &quot;uk_sites_prl&quot;
String privateKey = &quot;../../TestData/Keys/prl-d7-sftp-user-privatekey.pem&quot;
String remoteDir = &quot;/${StoreID}/Archive/&quot;

// File patterns to match
List&lt;String&gt; filePatterns = [&quot;*.TRX&quot;, &quot;*.H1&quot;, &quot;*.H2&quot;, &quot;*.A&quot;, &quot;*.P&quot;]

// Initialize JSch and session
JSch jsch = new JSch()
jsch.addIdentity(privateKey)

Session session = null
ChannelSftp channel = null

try {
    // Connect to the SFTP server
    session = jsch.getSession(username, host, port)
    session.setConfig(&quot;StrictHostKeyChecking&quot;, &quot;no&quot;)
    session.connect()
    
    channel = (ChannelSftp) session.openChannel(&quot;sftp&quot;)
    channel.connect()
    
    // Change to the target directory
    channel.cd(remoteDir)

    // Loop through each file pattern
    filePatterns.each { pattern -&gt;
        log.info(&quot;Looking for files matching: ${pattern}&quot;)

        // List files in the directory matching the current pattern
        Vector&lt;ChannelSftp.LsEntry&gt; files = channel.ls(pattern)

        if (files.isEmpty()) {
            log.info(&quot;No files matching ${pattern} found in ${remoteDir}&quot;)
        } else {
            files.each { file -&gt;
                String fileName = file.filename
                log.info(&quot;Deleting file: ${fileName}&quot;)
                channel.rm(fileName) // Delete the file
            }
            log.info(&quot;All files matching ${pattern} have been deleted.&quot;)
        }
    }
} catch (Exception e) {
    log.error(&quot;Error occurred during SFTP operation: ${e.message}&quot;, e)
} finally {
    // Disconnect from the SFTP server
    if (channel != null) channel.disconnect()
    if (session != null) session.disconnect()
}</stringProp>
        </JSR223Sampler>
