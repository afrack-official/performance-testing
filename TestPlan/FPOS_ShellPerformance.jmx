          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="NL_BS_FPOS_TRX_BuildTransactionFiles" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import java.io.*
import java.math.BigDecimal

// JSR223 Groovy script refactor of BS_FPOS_TRX_BuildTransactionFiles
// Ensures file-handle safety and precise BigDecimal arithmetic.

// Initialize transaction totals as BigDecimal
BigDecimal transactionTotalT1 = BigDecimal.ZERO
BigDecimal transactionTotalT2 = BigDecimal.ZERO

// Initialize sequence counters
def sno    = vars.get(&apos;sno&apos;)?.toInteger() ?: 0
def trasno = vars.get(&apos;trasno&apos;)?.toInteger() ?: 101

// Read dynamic variables from JMeter
def dateTimeTrx = vars.get(&apos;DateTimeTrx&apos;)
def storeID     = vars.get(&apos;StoreID&apos;)
def accDayNum   = vars.get(&apos;AccDayNum&apos;)
def seq         = vars.get(&apos;DayRepNr&apos;)
def counter     = vars.get(&apos;counter&apos;) ?: &apos;0&apos;
def paddedCounter = counter.padLeft(4, &apos;0&apos;)

// Define folder paths for templates and results
String folderPath        = &quot;../../TestData/FPOS_Templates/Prod/transactions/&quot;
String storeIDFolderPath = &quot;../../TestResults/NL/${storeID}/&quot;
File templateFolder      = new File(folderPath)
File storeIDFolder       = new File(storeIDFolderPath)
storeIDFolder.mkdirs()

// Build output filename and full path
def buildFilename = &quot;${paddedCounter}0${seq}.TRX&quot;
vars.put(&apos;btrxfile&apos;, buildFilename)
def buildFilePath = storeIDFolderPath + buildFilename

// Load template files
def transactionCount = vars.get(&apos;ppTrnxNum&apos;)?.toInteger() ?: 0
def files = templateFolder.listFiles()
if (!files) {
    log.warn(&quot;No transaction templates found in: ${folderPath}&quot;)
    return
}

// Write transactions safely using withWriter
new File(buildFilePath).withWriter(&apos;UTF-8&apos;) { writer -&gt;
    writer.writeLine(&apos;[START_FILE]&apos;)
    writer.writeLine(&apos;&apos;)
    writer.writeLine(&apos;[TRX]&apos;)

    files.take(transactionCount).eachWithIndex { file, index -&gt;
        def isEven    = (index % 2 == 0)
        def shiftNum  = isEven ? vars.get(&apos;ShiftRepNr_TillNo_2&apos;) : vars.get(&apos;ShiftRepNr_TillNo_1&apos;)
        def operatorId= isEven ? vars.get(&apos;T2OptrID&apos;)           : vars.get(&apos;T1OptrID&apos;)
        def tillNum   = isEven ? vars.get(&apos;T2TillID&apos;)          : vars.get(&apos;T1TillID&apos;)

        def currentSno = sno + index
        def newSeqNum  = currentSno + 1
        def seqString  = newSeqNum.toString().padLeft(4, &apos;0&apos;)
        def zeros      = &apos;&apos;.padLeft(7 - currentSno.toString().length(), &apos;0&apos;)
        def seqField   = &quot;0${seq}${tillNum}${zeros}&quot; + (newSeqNum - (index.intdiv(2)))

        file.eachLine(&apos;UTF-8&apos;) { line -&gt;
            def modified = line.replace(&apos;#SNO#&apos;, trasno.toString())
                               .replace(&apos;#DATI#&apos;, dateTimeTrx)
                               .replace(&apos;#STATION_NR#&apos;, storeID)
                               .replace(&apos;#ShiftID#&apos;, shiftNum)
                               .replace(&apos;#ACC_REP_NR#&apos;, accDayNum)
                               .replace(&apos;#OPID#&apos;, operatorId)
                               .replace(&apos;@TillID@&apos;, tillNum)
                               .replace(&apos;#AccDayNum#&apos;, &quot;0${accDayNum}&quot;)
                               .replace(&apos;#REFSEQ#&apos;, seqString)
                               .replace(&apos;#SEQ#&apos;, seqField)

            writer.writeLine(modified)

            if (modified.startsWith(&apos;SLSTOT&apos;)) {
                def parts = modified.split(&apos;=&apos;)
                if (parts.size() == 2) {
                    try {
                        BigDecimal value = parts[1].trim().toBigDecimal()
                        if (tillNum.contains(&apos;11&apos;)) transactionTotalT1 = transactionTotalT1.add(value)
                        if (tillNum.contains(&apos;12&apos;)) transactionTotalT2 = transactionTotalT2.add(value)
                    } catch (Exception e) {
                        log.error(&apos;Error parsing SLSTOT: &apos; + e.getMessage())
                    }
                }
            }
        }

        trasno++
    }

    // Update cumulative sums as BigDecimal
    BigDecimal sumT1 = vars.get(&apos;sumT1&apos;)?.toBigDecimal() ?: BigDecimal.ZERO
    BigDecimal sumT2 = vars.get(&apos;sumT2&apos;)?.toBigDecimal() ?: BigDecimal.ZERO
    sumT1 = sumT1.add(transactionTotalT1)
    sumT2 = sumT2.add(transactionTotalT2)
    vars.put(&apos;sumT1&apos;, sumT1.setScale(2, BigDecimal.ROUND_HALF_UP).toString())
    vars.put(&apos;sumT2&apos;, sumT2.setScale(2, BigDecimal.ROUND_HALF_UP).toString())

    vars.put(&apos;sno&apos;, (sno + transactionCount).toString())
    vars.put(&apos;trasno&apos;, trasno.toString())

    writer.writeLine(&apos;[END_TRX]&apos;)
    writer.writeLine(&apos;&apos;)
    writer.writeLine(&apos;[GENERIC_FILE_INFO]&apos;)
    writer.writeLine(&quot;CREATED=${new Date().format(&apos;yyyyMMddHHmmss&apos;)}&quot;)
    writer.writeLine(&apos;&apos;)
    writer.writeLine(&apos;[END_GENERIC_FILE_INFO]&apos;)
    writer.writeLine(&apos;&apos;)
    writer.writeLine(&apos;[END_FILE]&apos;)
}

log.info(&quot;Transaction build completed: ${buildFilePath}&quot;)
</stringProp>
          </JSR223Sampler>
