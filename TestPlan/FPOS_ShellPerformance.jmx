        String escapedThreadGroupName = threadGroupName.replace(&quot; &quot;, &quot;\\\\ &quot;).replace(&quot;,&quot;, &quot;\\\\,&quot;);
        String escapedStoreID = storeID.replace(&quot; &quot;, &quot;\\\\ &quot;).replace(&quot;,&quot;, &quot;\\\\,&quot;);

        // Create the line protocol data to send to InfluxDB
        String data = &quot;jmeter_failed_requests,application=jmeter,parent_sample_label=${escapedParentSampleLabel},request_name=${escapedRequestName},thread_group_name=${escapedThreadGroupName},store_id=${escapedStoreID} &quot; +
                      &quot;response_code=\&quot;${responseCode}\&quot;,response_message=\&quot;${responseMessage}\&quot;,response_time=${responseTime},assertion_messages=\&quot;${assertionMessages}\&quot;&quot;;

        // Send the data to InfluxDB using a POST request
        URL influxUrl = new URL(&quot;${influxHost}/api/v2/write?org=POS-Performance&amp;bucket=${influxDBName}&amp;precision=s&quot;);
        HttpURLConnection influxConn = (HttpURLConnection) influxUrl.openConnection();
        influxConn.setRequestMethod(&quot;POST&quot;);
        influxConn.setDoOutput(true);
        influxConn.setRequestProperty(&quot;Authorization&quot;, &quot;Token ${token}&quot;);
        influxConn.setRequestProperty(&quot;Content-Type&quot;, &quot;text/plain; charset=UTF-8&quot;);

        log.info(&quot;Sending data to InfluxDB: &quot; + data);

        // Send the data to InfluxDB
        influxConn.getOutputStream().write(data.getBytes(&quot;UTF-8&quot;));
        influxConn.getOutputStream().flush();
        influxConn.getOutputStream().close();

        // Get the response from InfluxDB
        int influxResponseCode = influxConn.getResponseCode();
        String influxResponseMessage = influxConn.getResponseMessage();

        // Handle InfluxDB response
        if (influxResponseCode != 204) {
            InputStream errorStream = influxConn.getErrorStream();
            String errorContent = errorStream != null ? new BufferedReader(new InputStreamReader(errorStream)).lines().collect(Collectors.joining(&quot;\n&quot;)) : &quot;&quot;;
            log.warn(&quot;Failed to send data to InfluxDB. Response code: &quot; + influxResponseCode + &quot;, message: &quot; + influxResponseMessage + &quot;, error: &quot; + errorContent);
        } else {
            log.info(&quot;Failed request data sent to InfluxDB successfully.&quot;);
        }
    } catch (Exception e) {
        log.error(&quot;Error while sending failed request data to InfluxDB: &quot; + e.message);
    }
}
