    // Check if the request failed or any assertions failed
    if (!currentSample.isSuccessful() || (assertionResults != null &amp;&amp; assertionResults.any { !it.isSuccessful() })) {
        String requestName = currentSample.getSampleLabel();
        String responseCode = currentSample.getResponseCode();
        String responseMessage = currentSample.getResponseMessage();
        long responseTime = currentSample.getTime();

        SampleResult parentResult = currentSample.getParent();
        String parentSampleLabel = &quot;No Parent Transaction&quot;;

        // Get parent transaction details if available
        while (parentResult != null) {
            String currentParentLabel = parentResult.getSampleLabel();
            if (currentParentLabel != null &amp;&amp; !currentParentLabel.isEmpty()) {
                parentSampleLabel = currentParentLabel;
            }
            parentResult = parentResult.getParent();
        }

        // Collect assertion failure details
        StringBuilder assertionMessages = new StringBuilder();
        if (assertionResults != null &amp;&amp; assertionResults.any { it.isFailure() }) {
