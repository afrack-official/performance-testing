    responseBody = responseBody.replace(&quot;\&quot;&quot;, &quot;&apos;&quot;).replace(&quot;\n&quot;, &quot;\\n&quot;).replace(&quot;\r&quot;, &quot;&quot;).trim()

    def failureTimeMs = System.currentTimeMillis()
    def readableFormatter = new SimpleDateFormat(&quot;MMM dd HH:mm:ss z yyyy&quot;)
    readableFormatter.setTimeZone(TimeZone.getTimeZone(&quot;Asia/Kolkata&quot;))
    def failureTimeReadable = readableFormatter.format(new Date(failureTimeMs)).replace(&quot;\&quot;&quot;, &quot;&apos;&quot;).replace(&quot;\n&quot;, &quot;\\n&quot;)

    sendToInfluxDB(
        requestName,
        responseCode,
        responseMessage,
        threadGroupName,
        storeID,
        failureTimeReadable,
        responseBody
    )
