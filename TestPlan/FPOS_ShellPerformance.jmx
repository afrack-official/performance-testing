          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Incrementing_ShiftSeq">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import org.apache.commons.io.FileUtils

// ---------- File Paths ----------
def firstCsv  = &quot;../../TestData/TestData_Prod/UK/Sites/Write_Sites_Shifts_In_Tills.csv&quot;
def secondCsv = &quot;../../TestData/TestData_Prod/UK/UK_FPOS_OptrTills.csv&quot;
def outputCsv = &quot;../../TestData/TestData_Prod/UK/UK_FPOS_StoreShiftSeq.csv&quot;

def f1 = new File(firstCsv)
def f2 = new File(secondCsv)
def out = new File(outputCsv)

log.info &quot;=== Increment + Merge (null222) starting ===&quot;

// ---------- Helpers ----------
def clean = { val -&gt;
    def v = (val == null ? &quot;&quot; : val.trim())
    return (v.isEmpty() || v.equalsIgnoreCase(&quot;null&quot;)) ? &quot;222&quot; : v
}
def incInt = { str -&gt;
    try {
        return (str?.trim()) ? (str.toInteger() + 1).toString() : &quot;222&quot;
    } catch(e) {
        return &quot;222&quot;
    }
}
def incLastUnderscoreNum = { s -&gt;
    if (!s) return &quot;222&quot;
    def idx = s.lastIndexOf(&apos;_&apos;)
    if (idx == -1) {
        // no underscore, just increment entire number
        return incInt(s)
    } else {
        def base = s.substring(0, idx + 1)
        def num  = s.substring(idx + 1)
        return base + incInt(num)
    }
}

// ---------- Read Files ----------
def rows1 = FileUtils.readLines(f1, &apos;UTF-8&apos;)
def rows2 = FileUtils.readLines(f2, &apos;UTF-8&apos;)

if (rows1.size() &lt;= 1 || rows2.size() &lt;= 1) {
    log.error &quot;One of the CSVs has no data rows!&quot;
    return
}

def data1 = rows1.subList(1, rows1.size())
def data2 = rows2.subList(1, rows2.size())

def maxRows = Math.min(data1.size(), data2.size())
if (data1.size() != data2.size()) {
    log.warn &quot;Row counts differ: first=${data1.size()} second=${data2.size()}&quot;
}

// ---------- Prepare output ----------
def merged = []
merged &lt;&lt; &quot;ShiftRepNr_TillNo_1,StoreID,DayRepNr,ShiftRepNr_TillNo_2,&quot; +
          &quot;T1TillID,T2TillID,T1OptrID,T2OptrID,T1POS,T2POS&quot;

// ---------- Process rows ----------
for (int i = 0; i &lt; maxRows; i++) {
    def c1 = data1[i].split(&quot;,&quot;, -1) // keep empty fields
    def c2 = data2[i].split(&quot;,&quot;, -1)

    // First CSV expected columns:
    // 0: ShiftRepNr_TillNo_1
    // 1: StoreID
    // 2: DayRepNr
    // 3: ShiftRepNr_TillNo_2

    def part1 = [
        incLastUnderscoreNum(clean(c1[0])),    // increment last num after underscore
        clean(c1[1]),                          // StoreID unchanged
        incInt(clean(c1[2])),                  // DayRepNr +1
        incInt(clean(c1[3]))                   // ShiftRepNr_TillNo_2 +1
    ]

    // Second CSV: T1TillID,T2TillID,T1OptrID,T2OptrID,T1POS,T2POS
    def part2 = c2.collect { clean(it) }

    merged &lt;&lt; (part1 + part2).join(&apos;,&apos;)
}

// ---------- Write output ----------
if (!out.parentFile.exists()) out.parentFile.mkdirs()
FileUtils.writeLines(out, &apos;UTF-8&apos;, merged)

log.info &quot; Merged ${maxRows} rows into ${out.absolutePath}&quot;
log.info &quot;=== Increment + Merge (null222) finished ===&quot;
</stringProp>
