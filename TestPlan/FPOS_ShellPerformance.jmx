def sendToInfluxDB(String requestName, String responseCode, String responseMessage,
                   String threadGroupName, String storeID,
                   String failureTimeReadable, String responseBody) {
    try {
        def influxHost = &quot;http://internal-observability-private-alb-1174906315.eu-central-1.elb.amazonaws.com:8086&quot;
        def token = &quot;7DU6Z-Vwpv6JTLGG39LHIbghlIokvu3coJSZENWrVDV-BTg_--d7u_arg5lpeiLNp5gPrxYaNQpxdtW3uTAikA==&quot;
        def bucket = &quot;jmeter_b2&quot;
        def org = &quot;POS-Performance&quot;

        def escape = { s -&gt; s?.replace(&quot; &quot;, &quot;\\\\ &quot;).replace(&quot;,&quot;, &quot;\\\\,&quot;) ?: &quot;unknown&quot; }

        def escapedRequest = escape(requestName)
        def escapedTG = escape(threadGroupName)
        def escapedStore = escape(storeID)
        def escapedFailureTime = failureTimeReadable.replace(&quot;\&quot;&quot;, &quot;&apos;&quot;).replace(&quot;\n&quot;, &quot;\\n&quot;)
        def escapedBody = responseBody.replace(&quot;\&quot;&quot;, &quot;&apos;&quot;).replace(&quot;\n&quot;, &quot;\\n&quot;)

        def line = &quot;jmeter_failed_requests,application=jmeter,request_name=${escapedRequest},thread_group_name=${escapedTG},store_id=${escapedStore} &quot; +
                   &quot;response_code=\&quot;${responseCode}\&quot;,response_message=\&quot;${responseMessage}\&quot;,failure_time_readable=\&quot;${escapedFailureTime}\&quot;,response_body=\&quot;${escapedBody}\&quot;&quot;

        def url = new URL(&quot;${influxHost}/api/v2/write?org=${org}&amp;bucket=${bucket}&amp;precision=s&quot;)
        def conn = (HttpURLConnection) url.openConnection()
        conn.setRequestMethod(&quot;POST&quot;)
        conn.setDoOutput(true)
        conn.setRequestProperty(&quot;Authorization&quot;, &quot;Token ${token}&quot;)
        conn.setRequestProperty(&quot;Content-Type&quot;, &quot;text/plain; charset=UTF-8&quot;)

        conn.outputStream.write(line.getBytes(&quot;UTF-8&quot;))
        conn.outputStream.flush()
        conn.outputStream.close()

        if (conn.getResponseCode() != 204) {
            def errorText = conn.getErrorStream()?.getText(&quot;UTF-8&quot;) ?: &quot;No error content&quot;
            log.warn(&quot;InfluxDB Error: ${conn.getResponseCode()} - ${conn.getResponseMessage()} - ${errorText}&quot;)
