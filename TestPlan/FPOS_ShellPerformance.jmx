			journal=journal+(&quot;,&quot;+element.get(&quot;JournalNo&quot;));
			log.info(&quot;journalNo: &quot;+element.get(&quot;JournalNo&quot;));
	}
	log.info(&quot;journalNo: &quot;+journal);
