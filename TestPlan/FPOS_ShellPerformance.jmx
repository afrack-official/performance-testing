			date= element.get(&quot;JournalDate&quot;);
			journal+=element.get(&quot;JournalNo&quot;);
		}
		boolean flag = true;
		String[] nos = journal.split(&quot;,&quot;);
		if(nos.length&gt;0)
			for(int j=0;j&lt;nos.length;j++)
				if(nos[j].equals(String.valueOf(element.get(&quot;JournalNo&quot;))))
					flag=false;
