import org.apache.jmeter.samplers.SampleResult;
import org.apache.jmeter.threads.JMeterContextService;
import java.net.HttpURLConnection;
import java.net.URL;
import java.io.BufferedReader;
import java.io.InputStream;
import java.io.InputStreamReader;
import java.util.stream.Collectors;

// Configuration: InfluxDB details
def influxHost = &quot;http://internal-observability-private-alb-1174906315.eu-central-1.elb.amazonaws.com:8086&quot;;
def influxDBName = &quot;jmeter_b2&quot;;
def token = &quot;7DU6Z-Vwpv6JTLGG39LHIbghlIokvu3coJSZENWrVDV-BTg_--d7u_arg5lpeiLNp5gPrxYaNQpxdtW3uTAikA==&quot;;

// Accessing the thread group information
def threadGroup = ctx.getThreadGroup();
def threadGroupName = threadGroup.getName();  // Thread group name
def threadGroupThreads = threadGroup.getNumberOfThreads();

// Initialize list for failed requests
def failedRequests = [];

// Current sample for the thread
def currentSample = ctx.getPreviousResult();

if (currentSample != null) {
    def assertionResults = currentSample.getAssertionResults();
    if (!currentSample.isSuccessful() || (assertionResults != null &amp;&amp; assertionResults.any { !it.isSuccessful() })) {
        String requestName = currentSample.getSampleLabel();
        String responseCode = currentSample.getResponseCode();
        String responseMessage = currentSample.getResponseMessage();
        long responseTime = currentSample.getTime();

        SampleResult parentResult = currentSample.getParent();
        String parentSampleLabel = &quot;No Parent Transaction&quot;;

        while (parentResult != null) {
            String currentParentLabel = parentResult.getSampleLabel();
            if (currentParentLabel != null &amp;&amp; !currentParentLabel.isEmpty()) {
                parentSampleLabel = currentParentLabel;
            }
            parentResult = parentResult.getParent();
        }

        // Collect assertion failure details
        StringBuilder assertionMessages = new StringBuilder();
        if (assertionResults != null &amp;&amp; assertionResults.any { it.isFailure() }) {
            assertionMessages.append(&quot;Assertion Failures: &quot;);
            assertionResults.each { assertionResult -&gt;
                if (assertionResult.isFailure()) {
                    assertionMessages.append(&quot;${assertionResult.getName()} - ${assertionResult.getFailureMessage()}; &quot;);
                }
            }
        } else {
            assertionMessages.append(&quot;No Assertion Failures&quot;);
        }

        String formattedData = &quot;Transaction: &quot; + parentSampleLabel + 
                               &quot;, Request: &quot; + requestName +
                               &quot;, Response Code: &quot; + responseCode +
                               &quot;, Response Message: &quot; + responseMessage +
                               &quot;, Response Time: &quot; + responseTime +
                               &quot;, Assertions: &quot; + assertionMessages.toString() +
                               &quot;, Thread Group: &quot; + threadGroupName;

        failedRequests.add(formattedData);
        log.info(&quot;----------------------------------- &quot; + formattedData);

        // Call the method to send failed data to InfluxDB
        sendToInfluxDB(influxHost, influxDBName, token, parentSampleLabel, requestName, responseCode, responseMessage, responseTime, assertionMessages.toString(), threadGroupName);
    }
}

// Define the sendToInfluxDB() method with updated escaping and thread group name
def sendToInfluxDB(String influxHost, String influxDBName, String token, String parentSampleLabel, String requestName, String responseCode, String responseMessage, long responseTime, String assertionMessages, String threadGroupName) {
    try {
        // Escape special characters in tags
        String escapedParentSampleLabel = parentSampleLabel.replace(&quot; &quot;, &quot;\\\\ &quot;).replace(&quot;,&quot;, &quot;\\\\,&quot;);
        String escapedRequestName = requestName.replace(&quot; &quot;, &quot;\\\\ &quot;).replace(&quot;,&quot;, &quot;\\\\,&quot;);
        String escapedThreadGroupName = threadGroupName.replace(&quot; &quot;, &quot;\\\\ &quot;).replace(&quot;,&quot;, &quot;\\\\,&quot;);

        String data = &quot;jmeter_failed_requests,application=jmeter,parent_sample_label=${escapedParentSampleLabel},request_name=${escapedRequestName},thread_group_name=${escapedThreadGroupName} &quot; +
                      &quot;response_code=\&quot;${responseCode}\&quot;,response_message=\&quot;${responseMessage}\&quot;,response_time=${responseTime},assertion_messages=\&quot;${assertionMessages}\&quot;&quot;;

        URL influxUrl = new URL(&quot;${influxHost}/api/v2/write?org=POS-Performance&amp;bucket=${influxDBName}&amp;precision=s&quot;);
        HttpURLConnection influxConn = (HttpURLConnection) influxUrl.openConnection();
        influxConn.setRequestMethod(&quot;POST&quot;);
        influxConn.setDoOutput(true);
        influxConn.setRequestProperty(&quot;Authorization&quot;, &quot;Token ${token}&quot;);
        influxConn.setRequestProperty(&quot;Content-Type&quot;, &quot;text/plain; charset=UTF-8&quot;);

        // Log data being sent for debugging
        log.info(&quot;Sending data to InfluxDB: &quot; + data);

        // Send the data
        influxConn.getOutputStream().write(data.getBytes(&quot;UTF-8&quot;));
        influxConn.getOutputStream().flush();
        influxConn.getOutputStream().close();

        int influxResponseCode = influxConn.getResponseCode();
        String influxResponseMessage = influxConn.getResponseMessage();

        // Handle InfluxDB response
        if (influxResponseCode != 204) {
            InputStream errorStream = influxConn.getErrorStream();
            String errorContent = errorStream != null ? new BufferedReader(new InputStreamReader(errorStream)).lines().collect(Collectors.joining(&quot;\n&quot;)) : &quot;&quot;;
            log.warn(&quot;Failed to send data to InfluxDB. Response code: &quot; + influxResponseCode + &quot;, message: &quot; + influxResponseMessage + &quot;, error: &quot; + errorContent);
        } else {
            log.info(&quot;Failed request data sent to InfluxDB successfully.&quot;);
        }
    } catch (Exception e) {
        log.error(&quot;Error while sending failed request data to InfluxDB: &quot; + e.message);
    }
}
