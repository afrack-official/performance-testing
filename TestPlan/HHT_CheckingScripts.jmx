import java.net.URL;
import java.io.OutputStreamWriter;
import java.util.ArrayList;
import java.util.Collections;

// InfluxDB connection parameters
String influxDBUrl = &quot;http://internal-observability-private-alb-1174906315.eu-central-1.elb.amazonaws.com:8086/api/v2/write?org=POS-Performance&amp;bucket=jmeter_b2&amp;precision=ms&quot;;
String token = &quot;7DU6Z-Vwpv6JTLGG39LHIbghlIokvu3coJSZENWrVDV-BTg_--d7u_arg5lpeiLNp5gPrxYaNQpxdtW3uTAikA==&quot;; // Replace with your InfluxDB token

// Retrieve the current sample result
def sampleResult = prev; // &apos;prev&apos; refers to the last executed sample result

// Function to get the top-level parent sampler, if it exists
def getTopLevelSampler(SampleResult result) {
    SampleResult current = result;
    while (current.getParent() != null) {
        current = current.getParent(); // Move up to the top-most parent sampler
    }
    return current;
}

// Function to calculate the cumulative response time and the 90th percentile of all child samplers
def calculateMetrics(SampleResult parentSample) {
    def totalResponseTime = 0;
    def childSamples = parentSample.getSubResults(); // Fetch all sub-results (child samplers)
    ArrayList&lt;Integer&gt; responseTimes = new ArrayList&lt;&gt;(); // List to store response times for percentile calculation

    if (childSamples != null &amp;&amp; childSamples.length &gt; 0) {
        log.info(&quot;Found &quot; + childSamples.length + &quot; child samplers under &quot; + parentSample.getSampleLabel());
        for (SampleResult childSample : childSamples) {
            log.info(&quot;Adding response time for child sampler: &quot; + childSample.getSampleLabel() + &quot; - Time: &quot; + childSample.getTime() + &quot; ms&quot;);
            int time = childSample.getTime();
            totalResponseTime += time;
            responseTimes.add(time); // Add time to the list for percentile calculation
        }
    } else {
        log.warn(&quot;No child samplers found under transaction controller: &quot; + parentSample.getSampleLabel());
        totalResponseTime = parentSample.getTime(); // Use parent time if no child samplers found
        responseTimes.add(totalResponseTime); // Add to list for percentile calculation
    }

    // Calculate the 90th percentile
    Collections.sort(responseTimes);
    int index90th = (int) Math.ceil(0.9 * responseTimes.size()) - 1;
    int percentile90th = responseTimes.get(index90th);

    return [totalResponseTime, percentile90th];
}

// Check if sampleResult is not null and attempt to find the top-level sampler
if (sampleResult != null) {
    // Get the top-level parent sampler (transaction controller in this case)
    def topLevelSampler = getTopLevelSampler(sampleResult);

    // Proceed with the dynamic label of the top-level transaction controller
    if (topLevelSampler != null) {
        def transactionLabel = topLevelSampler.getSampleLabel(); // Dynamically capture transaction label
        def (elapsedTime, percentile90th) = calculateMetrics(topLevelSampler); // Get cumulative response time and 90th percentile
        String status = sampleResult.isSuccessful() ? &quot;ok&quot; : &quot;failed&quot;; // Define status based on the sample result

        if (elapsedTime &gt; 0) {
            // Prepare data in InfluxDB line protocol format with &quot;status&quot; as a tag
            String influxData = &quot;jmeter_metrics,sampler=&quot; + transactionLabel.replaceAll(&quot; &quot;, &quot;_&quot;) +
                                &quot;,status=&quot; + status + &quot; elapsedTime=&quot; + elapsedTime + &quot;i,percentile90th=&quot; + percentile90th + &quot;i&quot;;
            log.info(&quot;========&quot; + influxData);

            try {
                // Open a connection to InfluxDB
                URL url = new URL(influxDBUrl);
                HttpURLConnection connection = (HttpURLConnection) url.openConnection();
                connection.setRequestMethod(&quot;POST&quot;);
                connection.setRequestProperty(&quot;Authorization&quot;, &quot;Token &quot; + token);
                connection.setRequestProperty(&quot;Content-Type&quot;, &quot;text/plain&quot;);
                connection.setDoOutput(true);

                // Send data
                OutputStreamWriter writer = new OutputStreamWriter(connection.getOutputStream());
                writer.write(influxData);
                writer.flush();
                writer.close();

                // Check response
                if (connection.getResponseCode() == 204) {
                    log.info(&quot;Metrics sent successfully to InfluxDB.&quot;);
                } else {
                    log.warn(&quot;Failed to send metrics to InfluxDB. Response code: &quot; + connection.getResponseCode());
                }
            } catch (Exception e) {
                log.error(&quot;Error sending metrics to InfluxDB: &quot; + e.getMessage());
            }
        } else {
            log.warn(&quot;No cumulative response time calculated. Possible issue with child sampler retrieval.&quot;);
        }
    } else {
        log.warn(&quot;No top-level sampler found.&quot;);
    }
} else {
    log.warn(&quot;No sample result found.&quot;);
}
