  },
  &quot;ProductName&quot;: &quot;Spur Cola 750ml&quot;,
  &quot;UserID&quot;: &quot;1177&quot;,
  &quot;SupplierProductID&quot;: &quot;&quot;,
  &quot;ItemQty&quot;: 0
}
&apos;&apos;&apos;

// Parse JSON
def parser = new JSONParser(JSONParser.MODE_PERMISSIVE)
def jsonObj = (JSONObject) parser.parse(jsonString)
def product = (JSONObject) jsonObj.get(&quot;product&quot;)
def supplierList = (JSONArray) product.get(&quot;SupplierList&quot;)

// Update fields
jsonObj.put(&quot;_id&quot;, &quot;${__Random(100000000000000,999999999999999)}&quot;)
