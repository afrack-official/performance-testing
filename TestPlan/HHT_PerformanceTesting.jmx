    &quot;CurrStock&quot;: &quot;188&quot;,
    &quot;NoOrders&quot;: false,
    &quot;OnOrder&quot;: &quot;0&quot;,
    &quot;OnOrderCases&quot;: &quot;0&quot;,
    &quot;OrderPackSize&quot;: &quot;12x1xEACH&quot;,
    &quot;PackSize&quot;: &quot;&quot;,
    &quot;ProductID&quot;: &quot;C0000005008&quot;,
    &quot;ProductName&quot;: &quot;Spur Cola 750ml&quot;,
    &quot;SellingPrice&quot;: 1,
    &quot;ShelfLifeDays&quot;: 0,
    &quot;SupplierID&quot;: &quot;C000000001&quot;,
    &quot;SupplierList&quot;: [
      {
        &quot;__type&quot;: &quot;HandheldSupplierID&quot;,
