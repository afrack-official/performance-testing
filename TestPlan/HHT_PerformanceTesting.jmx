              <stringProp name="script">import net.minidev.json.parser.JSONParser
import net.minidev.json.JSONObject
import net.minidev.json.JSONArray
import java.text.SimpleDateFormat
import org.apache.commons.math3.util.BigReal

def jsonString = &apos;&apos;&apos;
{
  &quot;_id&quot;: &quot;3fcdfe7f-688f-417e-84e9-5c90fac9881d&quot;,
  &quot;_modeIdentifier&quot;: &quot;WASTAGE&quot;,
  &quot;__type&quot;: &quot;HandheldTrans&quot;,
  &quot;AreaID&quot;: &quot;462&quot;,
  &quot;Barcode&quot;: &quot;230014&quot;,
  &quot;Qty&quot;: 3,
  &quot;Ref2&quot;: &quot;&quot;,
  &quot;TransDate&quot;: &quot;2023-11-27T10:42:55.426&quot;,
  &quot;TransType&quot;: 99,
  &quot;ProductID&quot;: &quot;C0000005008&quot;,
  &quot;SupplierID&quot;: &quot;C000000001&quot;,
  &quot;product&quot;: {
    &quot;__type&quot;: &quot;HandheldProductDetail&quot;,
    &quot;AvgSales&quot;: &quot;0&quot;,
    &quot;CaseCost&quot;: &quot;15&quot;,
    &quot;CurrStock&quot;: &quot;188&quot;,
    &quot;NoOrders&quot;: false,
    &quot;OnOrder&quot;: &quot;0&quot;,
    &quot;OnOrderCases&quot;: &quot;0&quot;,
    &quot;OrderPackSize&quot;: &quot;12x1xEACH&quot;,
    &quot;PackSize&quot;: &quot;&quot;,
    &quot;ProductID&quot;: &quot;C0000005008&quot;,
    &quot;ProductName&quot;: &quot;Spur Cola 750ml&quot;,
    &quot;SellingPrice&quot;: 1,
    &quot;ShelfLifeDays&quot;: 0,
    &quot;SupplierID&quot;: &quot;C000000001&quot;,
    &quot;SupplierList&quot;: [
      {
        &quot;__type&quot;: &quot;HandheldSupplierID&quot;,
        &quot;SupplierID&quot;: &quot;C000000001&quot;
      }
    ],
    &quot;UnitCost&quot;: 15,
    &quot;UnitInCase&quot;: 1
  },
  &quot;ProductName&quot;: &quot;Spur Cola 750ml&quot;,
  &quot;UserID&quot;: &quot;1177&quot;,
  &quot;SupplierProductID&quot;: &quot;&quot;,
  &quot;ItemQty&quot;: 0
}
&apos;&apos;&apos;

// Parse JSON
def parser = new JSONParser(JSONParser.MODE_PERMISSIVE)
def jsonObj = (JSONObject) parser.parse(jsonString)
def product = (JSONObject) jsonObj.get(&quot;product&quot;)
def supplierList = (JSONArray) product.get(&quot;SupplierList&quot;)

// Update fields
jsonObj.put(&quot;_id&quot;, &quot;${__Random(100000000000000,999999999999999)}&quot;)
jsonObj.put(&quot;AreaID&quot;, vars.get(&quot;NLHHTstoreid&quot;))
