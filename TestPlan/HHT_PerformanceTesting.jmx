        def bucket = &quot;jmeter_b2&quot;
        def org = &quot;POS-Performance&quot;

        def escape = { s -&gt; s?.replace(&quot; &quot;, &quot;\\\\ &quot;).replace(&quot;,&quot;, &quot;\\\\,&quot;) ?: &quot;unknown&quot; }

        def escapedRequest = escape(requestName)
        def escapedTG = escape(threadGroupName)
        def escapedStore = escape(storeID)
        def escapedFailureTime = failureTimeReadable.replace(&quot;\&quot;&quot;, &quot;&apos;&quot;).replace(&quot;\n&quot;, &quot;\\n&quot;)
