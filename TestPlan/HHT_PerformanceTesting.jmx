def productUnitCost = new BigReal(productUnitCostStr)
product.put(&quot;UnitCost&quot;, productUnitCost)

// Add SupplierList dynamically
for (int i = 1; i &lt;= 16; i++) {
    def id = vars.get(&quot;supplierListId&quot; + i)
    def type = vars.get(&quot;supplierListType&quot; + i)
    
