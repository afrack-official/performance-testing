jsonObj.put(&quot;Qty&quot;, 8)
jsonObj.put(&quot;TransDate&quot;, new SimpleDateFormat(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss.SSS&quot;).format(new Date()))
jsonObj.put(&quot;ProductID&quot;, vars.get(&quot;productProductId&quot;))
jsonObj.put(&quot;SupplierID&quot;, vars.get(&quot;productSupplierId&quot;))
jsonObj.put(&quot;ProductName&quot;, vars.get(&quot;productProductName&quot;))

// Product updates
product.put(&quot;__type&quot;, vars.get(&quot;productType&quot;))
