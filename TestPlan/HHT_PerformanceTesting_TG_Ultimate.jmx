def modifiedResponse = response.replaceFirst(/&quot;total&quot;:\s*\d+,/, &quot;&quot;).replaceFirst(/&quot;data&quot;:/, &apos;&quot;DEData&quot;:&apos;)

// Parse and format the JSON response without appending &quot;DeleteLabelView&quot;: false
def jsonResponseWithoutAppend = new JsonSlurper().parseText(modifiedResponse)
def formattedJsonWithoutAppend = JsonOutput.toJson(jsonResponseWithoutAppend)
def prettyJsonWithoutAppend = JsonOutput.prettyPrint(formattedJsonWithoutAppend)

// Store this version in a JMeter variable without the additional field
vars.put(&quot;modifiedResponseVariableWithoutAppend&quot;, prettyJsonWithoutAppend)

// Now, append `,&quot;DeleteLabelView&quot;: false` after the last closing square bracket `]`
def modifiedResponseWithAppend = modifiedResponse.replaceFirst(/\]/, &apos;],&quot;DeleteLabelView&quot;:false&apos;)

// Parse and format the JSON response with &quot;DeleteLabelView&quot;: false appended
def jsonResponseWithAppend = new JsonSlurper().parseText(modifiedResponseWithAppend)
def formattedJsonWithAppend = JsonOutput.toJson(jsonResponseWithAppend)
def prettyJsonWithAppend = JsonOutput.prettyPrint(formattedJsonWithAppend)

// Store this version in a JMeter variable with the additional field
vars.put(&quot;modifiedResponseVariableWithAppend&quot;, prettyJsonWithAppend)
